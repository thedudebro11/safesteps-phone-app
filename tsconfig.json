{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "moduleSuffixes": [".native", ".ios", ".android", ".web", ""],
  
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
  "**/*.ts",
  "**/*.tsx",
  ".expo/types/**/*.ts",
  "expo-env.d.ts",
  "expo-router"
]

}
